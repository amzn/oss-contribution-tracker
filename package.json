    "mockery": "^1.7.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.15.1",
    "pg-monitor": "^0.4.5",
    "popper.js": "^1.12.9",
    "prettier": "^1.12.1",
    "sass-loader": "^6.0.6",
    "selenium-webdriver": "^3.0.1",
    "source-map-support": "^0.4.0",
    "ts-loader": "^3.5.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.12.0",
